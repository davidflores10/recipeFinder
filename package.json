{
  "name": "vue-vite-app",
  "version": "1.0.0",
  "description": "A Vue.js project using Vite as the build tool.",
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "npm run test:unit:coverage && rm -r coverage/tmp/",
    "test:unit": "vitest --environment jsdom",
    "test:unit:watch": "vitest run --environment jsdom",
    "test:unit:coverage": "vitest run --environment jsdom --coverage"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "pinia": "^3.0.2",
    "vue": "^3.2.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.0.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^4.0.0",
    "vite": "^2.0.0",
    "vitest": "^3.1.1"
  },
  "license": "MIT"
}
