{
  "name": "vue-vite-app",
  "version": "1.0.0",
  "type": "module",
  "description": "A Vue.js project using Vite as the build tool.",
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit:coverage && rm -r coverage/tmp/",
    "test:unit": "vitest --environment jsdom",
    "test:unit:watch": "vitest run --environment jsdom",
    "test:unit:coverage": "vitest run --environment jsdom --coverage"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "pinia": "^2.1.7",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.0",
    "vite": "^5.4.18",
    "vitest": "^3.1.1"
  },
  "license": "MIT"
}
